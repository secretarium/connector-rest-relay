        "@typescript-eslint/parser": "4.21.0",
        "eslint": "7.23.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jest": "24.3.4",
        "nodemon": "2.0.7",
        "ts-node": "9.1.1",
        "typescript": "4.2.4"
